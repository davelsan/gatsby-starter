{
  "name": "gatsby-starter",
  "private": true,
  "description": "A Gatsby starter with ESLint, Prettier, and TypeScript support",
  "version": "0.1.0",
  "author": {
    "name": "David Velasco"
  },
  "license": "0BSD",
  "repository": {
    "type": "git",
    "url": "https://github.com/davelsan/gatsby-starter"
  },
  "bugs": {
    "url": "https://github.com/davelsan/gatsby-starter/issues"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint --ignore-path .gitignore . --ext js,jsx,ts,tsx",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "tscheck": "tsc",
    "tscheck:watch": "tsc --watch"
  },
  "dependencies": {
    "gatsby": "^2.25.3",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-manifest": "^2.5.2",
    "gatsby-plugin-offline": "^3.3.3",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.7.1",
    "gatsby-source-filesystem": "^2.4.2",
    "gatsby-transformer-remark": "^2.10.0",
    "gatsby-transformer-sharp": "^2.5.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.3.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-postcss": "^3.1.0",
    "postcss": "^8.1.7",
    "postcss-nested": "^5.0.1",
    "prettier": "2.2.1",
    "tailwindcss": "^2.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
